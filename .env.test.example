# PageCrawl Integration Test Configuration
# Copy this file to .env.test and fill in your test credentials

# Required: API key for authentication
PAGECRAWL_TEST_API_KEY=your-api-key-here

# Required: Workspace ID to use for tests
PAGECRAWL_TEST_WORKSPACE_ID=your-workspace-id-here

# Optional: Base URL (defaults to https://pagecrawl.test)
PAGECRAWL_TEST_BASE_URL=https://pagecrawl.test
